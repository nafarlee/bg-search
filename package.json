{
  "scripts": {
    "test": "shadow-cljs compile test && node test.js",
    "repl": "shadow-cljs node-repl",
    "deps": "shadow-cljs classpath",
    "release:pull-games": "shadow-cljs release pull-games && esbuild pull-games.js --bundle --platform=node --target=node20 --external:pg-native --outfile=pull-games.dist.js",
    "compile:pull-games": "shadow-cljs compile pull-games",
    "release:pull-plays": "shadow-cljs release pull-plays && esbuild pull-plays.js --bundle --platform=node --target=node20 --external:pg-native --outfile=pull-plays.dist.js",
    "compile:pull-plays": "shadow-cljs compile pull-plays",
    "release:set-game-id-cliff": "shadow-cljs release set-game-id-cliff && esbuild set-game-id-cliff.js --bundle --platform=node --target=node20 --external:pg-native --outfile=set-game-id-cliff.dist.js",
    "compile:set-game-id-cliff": "shadow-cljs compile set-game-id-cliff",
    "release:app": "shadow-cljs release app && esbuild app.js --bundle --platform=node --target=node20 --external:pg-native --outfile=app.dist.js",
    "compile:app": "shadow-cljs compile app"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "shadow-cljs": "^3.3.4"
  },
  "dependencies": {
    "express": "^4.22.1",
    "fast-xml-parser": "^5.3.6",
    "parsimmon": "^1.18.1",
    "pg": "^8.16.3"
  }
}
